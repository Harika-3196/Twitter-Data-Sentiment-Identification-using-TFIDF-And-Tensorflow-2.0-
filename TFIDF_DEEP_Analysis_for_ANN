   "source": [
    "import re\n",
    "\n",
    "\n",
    "\n",
    "def tweet_to_words(tweet):\n",
    "    text_only=re.sub(\"[^a-zA-Z]\",\" \",tweet)\n",
    "    words=text_only.lower().split()\n",
    "    meaningful_words= [w for w in words if not w in stops]\n",
    "    return (\" \".join(meaningful_words))"
   ]
  },
